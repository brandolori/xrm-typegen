{
  "name": "xrm-typegen",
  "version": "1.0.0",
  "description": "A TypeScript Declaration Generator for Dynamics 365 Forms",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "engines": {
    "node": "^16.0.0"
  },
  "scripts": {
    "build": "tsc && npm run template",
    "template": "./node_modules/.bin/copyfiles ./src/**/*.hbs ./dist/ --up 1"
  },
  "author": "Lorenzo Bartolini",
  "dependencies": {
    "adal-node": "^0.2.2",
    "commander": "^7.1.0",
    "handlebars": "^4.7.7",
    "node-fetch": "^2.6.1",
    "node-localstorage": "^2.1.6"
  },
  "devDependencies": {
    "@types/node": "^12.19.16",
    "@types/node-fetch": "^2.5.8",
    "@types/sinon": "^9.0.10",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "sinon": "^9.2.4",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist/**/*"
  ]
}